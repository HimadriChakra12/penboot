			"Remove": [
				"Amazon.com",
				"eBay",
                "Bing",
                "DuckDuckGo"
			],
			"Default": "Brave",
			"Add": [
				{
					"Name": "DuckDuckGo Lite",
					"Description": "Minimal, ad-free version of DuckDuckGo",
					"Alias": "",
