				{
					"Name": "MetaGer",
					"Description": "MetaGer: Privacy Protected Search & Find",
					"Alias": "",
					"Method": "GET",
					"URLTemplate": "https://metager.org/meta/meta.ger3?eingabe={searchTerms}"
				},
				{
					"Name": "StartPage",
					"Description": "The world's most private search engine",
					"Alias": "sp",
					"Method": "GET",
					"URLTemplate": "https://www.startpage.com/sp/search?query={searchTerms}"
				},
				{
					"Name": "Brave",
					"Description": "The world's most private search engine",
					"Alias": "bbg",
					"Method": "GET",
					"URLTemplate": "https://search.brave.com/search?q={searchTerms}"
				},
				{
					"Name": "YouTube",
					"Description": "Search On Youtube",
					"Alias": "yt",
					"Method": "GET",
					"URLTemplate": "https://www.youtube.com/results?search_query={searchTerms}"
				},
				{
					"Name": "GetIntoPC",
					"Description": "Getintopc Search",
					"Alias": "gt",
					"Method": "GET",
					"URLTemplate": "https://getintopc.com/?s={searchTerms}"
